# ==========================================
# Angular / Node / Frontend .gitignore
# ==========================================

# =============================
# Compiled output
# =============================
/dist/
/tmp/
/out-tsc/
/bazel-out/
/build/

# Angular cache
/.angular/cache

# =============================
# Node
# =============================
/node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# =============================
# Environment files (IMPORTANT)
# =============================
.env
.env.*
!.env.example

src/environments/environment.local.ts
src/environments/environment.prod.local.ts

# =============================
# IDEs and editors
# =============================

# JetBrains / IntelliJ
.idea/

# Eclipse
.project
.classpath
.settings/

# Cloud9
.c9/

# VS Code
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json

# VS Code cache & history
.history/
*.code-workspace

# =============================
# Testing / Coverage
# =============================
/coverage/
/test-results/
testem.log

# =============================
# Logs
# =============================
*.log

# =============================
# Misc
# =============================
.sass-cache/
/connect.lock
/libpeerconnection.log
/typings/

# =============================
# Docker (if used)
# =============================
docker-compose.override.yml
*.tar

# =============================
# OS files
# =============================
.DS_Store
Thumbs.db

# =============================
# Temp files
# =============================
*.swp
*.swo
*.tmp
